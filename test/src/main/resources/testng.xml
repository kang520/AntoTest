<?xml version="1.0" encoding="UTF-8" ?>

<suite name="用户管理系统测试套件">
    <test name="用户管理系统测试用例">
        <classes>
            <class name="com.kang.cases.LoginTest">
                <methods>
                    <include name="loginTrue"/>
                    <include name="loginFalse"/>
                </methods>
            </class>
            <class name="com.kang.cases.AddUserTest">
                <methods>
                    <include name="addUser"/>
                </methods>
            </class>
            <class name="com.kang.cases.GetUserInfoTest">
                <methods>
                    <include name="getUserInfoTest"/>
                </methods>
            </class>
            <class name="com.kang.cases.GetUserListTest">
                <methods>
                    <include name="getUserListInfo"/>
                </methods>
            </class>
            <class name="com.kang.cases.UpdateUserInfoTest">
                <methods>
                    <include name="updateUserInfo"/>
                    <include name="deleteUserInfo"/>
                </methods>
            </class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.kang.config.ExtentTestNGIReporterListener"></listener>
    </listeners>
</suite>